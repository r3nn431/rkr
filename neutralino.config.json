{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.r3nn431.rkr",
  "version": "0.0.1.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "filesystem.*",
    "debug.*",
    "os.*",
    "storage.*",
    ".*"
  ],
  "dataLocation": "app",
  "storageLocation": "app",
  "globalVariables": {
  },
  "modes": {
    "window": {
      "title": "RKR",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": true,
      "alwaysOnTop": false,
      "icon": "resources/favicon.ico",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": false,
      "transparent": true
    },
    "browser": {
      "globalVariables": {
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "applicationName": "RKR",
  "author": "r3nn431",
  "description": "ROLE KILL REST",
  "copyright": "Copyright Â© r3nn431 2025. All rights reserved.",
  "applicationIcon": "resources/icon.png",
  "cli": {
    "binaryName": "rkr",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.1.0",
    "clientVersion": "6.1.0",
    "debugEnabled": false,
    "copyItems": ["/README.md", "/CHANGELOG.txt"]
  }
}